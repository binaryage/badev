// PrefixedClassAliases.h
//
//    Contains all the compatibility alias declarations used

#import "BaClassPrefix.h"

<%=
  decls = @classes.dup.reverse
  decls.reject! { |klass| klass =~ /BA_PREFIXED_CLASS_ALIAS/ }
  decls.map! { |klass| "BA_PREFIXED_CLASS_SUPPORT(#{klass});" }
  decls.join("\n")
%>