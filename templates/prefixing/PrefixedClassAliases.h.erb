// PrefixedClassAliases.h
//
//    Contains all the compatibility alias declarations used

#import "BaClassPrefix.h"

#pragma clang diagnostic push
#pragma clang diagnostic ignored "-Wmissing-declarations"

<%=
  decls = @classes.dup
  decls.reject! { |klass| klass =~ /BA_PREFIXED_CLASS_ALIAS/ }
  decls.map! { |klass| "BA_PREFIXED_CLASS_ALIAS(#{klass});" }
  decls.join("\n")
%>

#pragma clang diagnostic pop